@model Note

<h3 class="my-5">
    Note Details
</h3>

<div class="row gx-5">
    
    <div class="col-12">
        
        <h5 class="mb-4">@Model.Content</h5>

       
        <p class="text-muted"><strong>Uploaded By:</strong> @Model.UploadedBy</p>

        
        <div class="mt-4">
            <a class="btn btn-primary" asp-action="Grid">Back to notes</a>
        </div>
        
    </div>
</div>
